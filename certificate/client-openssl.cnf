[req]
default_bits = 4096
prompt = no
default_md = sha256
req_extensions = req_ext
distinguished_name = dn

[dn]
C = IN
ST = YourState
L = YourCity
O = YourOrganization
OU = YourOrganizationalUnit
CN = client

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = client
# You generally don't need IP addresses for clients